{
  "name": "preprocessor",
  "description": "A language agnostic, C-like preprocessor for D",
  "copyright": "Copyright Â© 2023, Mike Bierlee",
  "authors": ["Mike Bierlee"],
  "license": "MIT",
  "targetPath": "bin",
  "configurations": [
    {
      "name": "build",
      "targetType": "library"
    },
    {
      "name": "unittest",
      "targetType": "library"
    },
    {
      "name": "simpleCExample",
      "targetType": "executable",
      "targetName": "simpleCExample",
      "sourcePaths": ["examples/simpleC"],
      "importPaths": ["source"],
      "targetPath": "bin/examples/c",
      "stringImportPaths": ["examplesource/c"]
    },
    {
      "name": "advancedCExample",
      "targetType": "executable",
      "targetName": "advancedCExample",
      "sourcePaths": ["examples/advancedC"],
      "importPaths": ["source"],
      "targetPath": "bin/examples/c",
      "stringImportPaths": ["examplesource/c"]
    }
  ]
}
